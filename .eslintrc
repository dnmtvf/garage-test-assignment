{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
        "browser": true,
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "arrow-body-style": ["error", "as-needed", { "requireReturnForObjectLiteral": true }],
  },
  "plugins": [
    "jest"
  ]
}
